{
    "manifest_version": 2,
    "name": "TabCloser",
    "description": "close tabs",
    "version": "1.0",
    "browser_action": {
        "default_icon": "image1.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}