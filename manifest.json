{
    "manifest_version": 3,
    "name": "TabCloser",
    "description": "close tabs",
    "version": "1.0",
    "action": {
        "default_icon": "image1.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
        
    ],
    "host_permissions":[

        "*://*/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self' ; object-src 'self'"
    }
    
}